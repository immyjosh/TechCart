<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:local="clr-namespace:TechCart" x:Class="TechCart.TechCartPage">
    <StackLayout x:Name="up">
        <Frame>
            <Image Source="https://png.icons8.com/color/260/rgb-circle-1.png" Aspect="Fill" WidthRequest="2"/>
        </Frame>
        <StackLayout.GestureRecognizers>
            <TapGestureRecognizer Tapped="Handle_Tapped1" />
        </StackLayout.GestureRecognizers>
        <StackLayout x:Name="down">
            <Label Text="down" />
            <StackLayout.GestureRecognizers>
                <TapGestureRecognizer Tapped="Handle_Tapped2" />
            </StackLayout.GestureRecognizers>
        </StackLayout>
    </StackLayout>
</ContentPage>